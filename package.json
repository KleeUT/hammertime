{
  "name": "hammertime",
  "version": "3.1.0",
  "description": "U Can't Touch This",
  "main": "hammertime.js",
  "scripts": {
    "deploy": "node_modules/serverless/bin/serverless deploy --verbose",
    "destroy": "node_modules/serverless/bin/serverless remove --verbose",
